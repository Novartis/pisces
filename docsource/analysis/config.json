{
    "human": {
        "downloads": "$HOME/pisces/downloads",
        "index_dir": "$HOME/pisces/sra_test",
        "default": "gencode_basic",
        "index": {
            "gencode_basic": {
                "gtfs": ["ftp://ftp.ebi.ac.uk/pub/databases/gencode/Gencode_human/release_32/gencode.v32.basic.annotation.gtf.gz"],
                "fastas": ["http://hgdownload.soe.ucsc.edu/goldenPath/hg38/bigZips/hg38.fa.gz"],
                "extra_fastas": [],
                "options": {
                    "-k": 31,
                    "infer_features": false
                }
            },
            "gencode_basic_nomask": {
                "gtfs": ["ftp://ftp.ebi.ac.uk/pub/databases/gencode/Gencode_human/release_32/gencode.v32.basic.annotation.gtf.gz"],
                "fastas": ["http://hgdownload.soe.ucsc.edu/goldenPath/hg38/bigZips/hg38.fa.gz"],
                "extra_fastas": [],
                "options": {
                    "-k": 31,
                    "infer_features": false,
                    "masked": false
                }
            },
            "gencode_basic_exonic": {
                "gtfs": ["ftp://ftp.ebi.ac.uk/pub/databases/gencode/Gencode_human/release_32/gencode.v32.basic.annotation.gtf.gz"],
                "fastas": ["http://hgdownload.soe.ucsc.edu/goldenPath/hg38/bigZips/hg38.fa.gz"],
                "extra_fastas": [],
                "options": {
                    "-k": 31,
                    "infer_features": false,
                    "unprocessed_transcripts": false,
                    "intergenes": false
                }
            },
            "gencode_basic_exonic_nomask": {
                "gtfs": ["ftp://ftp.ebi.ac.uk/pub/databases/gencode/Gencode_human/release_32/gencode.v32.basic.annotation.gtf.gz"],
                "fastas": ["http://hgdownload.soe.ucsc.edu/goldenPath/hg38/bigZips/hg38.fa.gz"],
                "extra_fastas": [],
                "options": {
                    "-k": 31,
                    "infer_features": false,
                    "unprocessed_transcripts": false,
                    "intergenes": false,
                    "masked": false
                }
            }
        }
    }
}
